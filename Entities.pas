type
	TEntList = class
	private
		FList: TList;
		function GetCode(const Name: string): TEntityInfo;
	public
		constructor Create;
		property Items[const Name: string]: TEntityInfo read GetCode; default;
	end;

var
	EntityList: TEntList;

function EntCompare(Ent1, Ent2: Pointer): Integer;
begin
	//Entity names are case sensitive
	Result := CompareStr(PEntityInfo(Ent1).Name, PEntityInfo(Ent2)^.Name)
end;

constructor TEntList.Create;
var
	I: Integer;
begin
	inherited Create;

	FList := TList.Create;
	FList.Capacity := Length(EntTab);
	for I := 0 to High(EntTab) do
		FList.Add(@EntTab[I]);
	FList.Sort(EntCompare);
end;

function TEntList.GetCode(const Name: string): TEntityInfo;
var
	I, L, U, Cmp: Integer;
	ent: PEntityInfo;
begin
	L := 0;
	U := FList.Count - 1;
	while L <= U do
	begin
		I := (L + U) div 2;
		Cmp := CompareStr(Name, PEntityInfo(FList.Items[I])^.Name);
		if Cmp = 0 then
		begin
			ent := PEntityInfo(FList.Items[i]);
			if Length(ent.Code) > 1 then
			begin
				OutputDebugString(PChar('Entity '+Name+' contains more than one code'));
				if IsDebuggerPresent then
					DebugBreak;
			end;
			Result := ent^;
			Exit
		end;
		if Cmp < 0 then
			U := I - 1
		else
			L := I + 1
	end;
	Result := Default_EntityInfo;
end;

function TryGetEntityInfo(const Name: string; out EntityInfo: TEntityInfo): Boolean;
begin
	EntityInfo := EntityList.Items[Name];
	if EntityInfo.Name <> '' then
	begin
		Result := True;
	end
	else
	begin
		Result := False;
		EntityInfo := Default_EntityInfo;
	end;
end;

function GetEntValue(const Name: string): UnicodeString;
var
	entityInfo: TEntityInfo;
begin
	Result := '';

	if not TryGetEntityInfo(Name, {out}entityInfo) then
		Exit;

	Result := UCS4StringToUnicodeString(entityInfo.Code);
end;

